<div class="reservoirPercentExceedanceBucket"> 
    <% if (comp === 1) { %>
        <h5 class="greentext"> ⬆️ <%= title %>: </h5>
    <% } else if (comp === -1) { %>
        <h5 class="redtext"> ⬇️ <%= title %>: </h5>
    <% } else { %>
        <h5> ⏺️ <%= title %>: </h5>
    <% } %>
    <!-- <p> <%= data[0].prob %>%:  <%= data[0].val %> </p>
    <p> <%= data[1].prob %>%:  <%= data[1].val %> </p>
    <p> <%= data[2].prob %>%:  <%= data[2].val %> </p> -->

    <div class="reservoirBucketElementContainer">
        <div style="grid-row: 1 / -1; margin-top: 5px;">
            <div style="display: grid; height: 50px; width: 50px; border: 2px solid black; grid-template-columns: 1fr; grid-template-rows:  
            <%= String(1 - data[4].val) + 'fr' %> 
            <%= String(data[3].val - data[4].val) + 'fr' %> 
            <%= String(data[2].val - data[3].val) + 'fr' %> 
            <%= String(data[1].val - data[2].val) + 'fr' %> 
            <%= String(data[0].val - data[1].val) + 'fr' %>
            <%= String(data[0].val) + 'fr' %>;">
                <div id="p0bucket"></div>
                <div id="p10bucket"></div>
                <div id="p30bucket"></div>
                <div id="p50bucket"></div>
                <div id="p70bucket"></div>
                <div id="p90bucket"></div>
            </div>
        </div>
        <div class="measureBar"> </div>
        <div class="measureText"> <%= Math.round(cap) %>AF </div>
        <div class="measureBar"> </div>
        <div class="measureText"> <%= Math.round(cap * 3 / 4) %>AF </div>
        <div class="measureBar"> </div>
        <div class="measureText"> <%= Math.round(cap / 2) %>AF </div>
        <div class="measureBar"></div>
        <div class="measureText"> <%= Math.round(cap / 4) %>AF </div>
    
    </div>

</div>
