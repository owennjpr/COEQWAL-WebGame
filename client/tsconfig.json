{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    // "baseUrl": "./src",
    "outDir": "./dist",
    "allowJs": true, 

  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
